{
  "name": "caouecs/sirtrevorjs",
  "description": "Sir Trevor JS in Laravel project",
  "keywords": ["laravel", "sir trevor js", "sir-trevor-js", "lpm"],
  "license": "MIT",
  "authors": [
    {
      "name": "caouecs",
      "email": "caouecs@caouecs.net"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "^5 || ^6",
    "erusev/parsedown-extra": "~0.7",
    "thujohn/twitter": "~2.0",
    "laravelcollective/html": "^5 || ^6"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.*"
  },
  "autoload": {
    "psr-0": {
      "Caouecs\\Sirtrevorjs\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "test"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Caouecs\\Sirtrevorjs\\SirtrevorjsServiceProvider"
      ]
    }
  }
}
